{
  "name": "coterminous-styled",
  "version": "1.0.0",
  "description": "Reusable components for React projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/otttooming/coterminous-styled.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "docs": "NODE_ENV=production webpack -p",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "tsc -p tsconfig.dist.json",
    "clean": "rm -rf dist",
    "dist": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run dist"
  },
  "author": "Ott Tooming",
  "license": "ISC",
  "dependencies": {
    "downshift": "1.31.14",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "styled-components": "3.2.6"
  },
  "devDependencies": {
    "@storybook/react": "3.4.2",
    "@types/babel-core": "6.25.3",
    "@types/prettier": "1.12.0",
    "@types/react": "16.3.12",
    "@types/react-dom": "16.0.5",
    "@types/storybook__addon-actions": "3.0.3",
    "@types/storybook__addon-links": "3.3.0",
    "@types/storybook__react": "3.0.7",
    "babel-core": "6.26.2",
    "prettier": "1.12.1",
    "source-map-loader": "0.2.3",
    "ts-loader": "3.5.0",
    "typescript": "2.8.3"
  }
